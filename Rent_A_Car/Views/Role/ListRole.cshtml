@page
@model List<string>;
@{
    List<string> a = Model;
    ViewBag.Title = "List Car";
    Layout = "~/Views/Role/_RoleLayout.cshtml";
}

<h1>Roles</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Role Name</th>
            </tr>
        </thead>
        <tbody>
        @if (a.Any())
        {
            foreach (var role in a)
            {
                <tr>
                    <td>@role</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td>No roles available.</td>
            </tr>
        }
        </tbody>
    </table>

